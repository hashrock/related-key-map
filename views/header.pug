div(class="header-box" onclick="")
  .title-text-box
    h1(class=`font-style-${current_language_id}`)
      a(href=`/?lang=${current_language_id}`)= title
  div(class="language-box" ontouchstart="")
    a
      i(class=`fas fa-globe-asia` aria-hidden="true")
      span.current-language= current_language
    ul(class="items")
      each lang_name, lang_id in language_list
        li
          a(class=`lang-${lang_id} font-style-${lang_id}`)
            = `${lang_name}`

